{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Customer register payload",
  "type": "object",
  "required": ["emailId", "password", "firstName", "lastName"],
  "properties": {
    "emailId": {
      "type": "string",
      "format": "email",
      "description": "User account email address"
    },
    "password": {
      "type": "string",
      "minLength": 1,
      "description": "Password"
    },
    "firstName": {
      "type": "string",
      "minLength": 5,
      "description": "First Name"
    },
    "lastName": {
      "type": "string",
      "minLength": 5,
      "description": "Last Name"
    }
  }
}
