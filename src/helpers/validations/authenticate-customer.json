{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Signup request payload",
  "type": "object",
  "required": ["emailId", "password"],
  "properties": {
    "emailId": {
      "type": "string",
      "format": "email",
      "description": "User account email address"
    },
    "password": {
      "type": "string",
      "minLength": 1,
      "description": "Keys to the kingdom"
    }
  }
}
